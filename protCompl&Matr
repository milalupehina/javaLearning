class Matrix {
  private int[][] add(int[][] a, int[][] b) {
    int[][] c = new int[3][4];
    for ( int i = 0; i < 3; ++i ) {
      for ( int j = 0; j < 4; ++j ) {
        c[i][j] = a[i][j] + b[i][j];
      }
    }

    return c;
  }

  private int[][] multiply(int[][] a, int[][] b) {
    int[][] c = new int[4][5];


    return c;

  }

