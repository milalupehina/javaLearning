  public static void main(String[] args) {
    int[][] matrAAdd = new int[3][4];
    matrAAdd = { {19,  20,  5,   3},
                 {-2, -13, 15,  -6},
                 { 6,  17,  3, -10}  };

    int[][] matrBAdd = new int[3][4];
    matrBAdd = { {-7, 20, -8, 12},
                 {-7, 10, 18, -1},
                 {-3, 10, 11, 11}  };

    int[][] matrCAdd = new int[3][4];

    matrCAdd = add(matrAAdd, matrBAdd);



    int[][] matrAMult = new int[4][3];
    matrAMult = { {-11, 20, 16},
                  {  8, 11, -1},
                  { 20, 10, 16},
                  { -8,  5, -9} };

    int[][] matrBMult = new int[3][5];
    matrBMult = { {-6,   8, -18,   6,  12},
                  { 3, -13,   2,   4, -16},
                  {-5,  -9,   9, -16,  16}  };

    int[][] matrCMult = new int[4][5];

    matrCMult = multiply(matrAMult, matrBMult);



    int[][] matrATran = new int[5][6];
    matrATran = { {  7, -14,   3,   6,  -1,   6},
                  {-16,  18,   8,  16,   8,  -7},
                  { -8, -14,  18, -15,  18,   5},
                  {-12,  -6, -15, -11, -19, -13},
                  { 10,   8,  -7, -11, -19, -18}  };
